{% extends "_layout.njk" %}
{% block layoutBody %}


<div class="container prose mx-auto py-12">

  <h1>{{ config.title }}</h1>
      {{ content | safe }}
  <div>
    <h3>Blog list</h3>
    {% for post in collections.posts %}
    <div>
    <a href="{{ post.url }}">{{ post.data.title}}</a>
    </div>
      {% endfor %}

  </div>

  {% include "./_footer.njk" %}
</div>
{% endblock %}